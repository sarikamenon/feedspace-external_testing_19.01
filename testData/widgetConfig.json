{
    "page": {
        "url": "https://rabbiabe.com/",
        "timeout": 30000
    },
    "widgets": [
        {
            "type": "VerticalScroll",
            "uiRules": {
                "minReviews": 1,
                "hasVerticalScroll": true
            }
        },
        {
            "type": "Carousel",
            "uiRules": {
                "minReviews": 1,
                "hasNavigation": true,
                "autoScroll": false
            }
        },
        {
            "type": "Masonry",
            "uiRules": {
                "minReviews": 1,
                "hasLoadMore": true,
                "gridLayout": true
            }
        },
        {
            "type": "StripSlider",
            "uiRules": {
                "minReviews": 1,
                "autoScroll": true
            }
        },
        {
            "type": "AvatarGroup",
            "uiRules": {
                "minAvatars": 1,
                "hasNavigation": false
            }
        },
        {
            "type": "AvatarSlider",
            "uiRules": {
                "minReviews": 1,
                "hasNavigation": true
            }
        },
        {
            "type": "HorizontalScroll",
            "uiRules": {
                "minReviews": 1,
                "hasHorizontalScroll": true
            }
        },
        {
            "type": "FloatingCards",
            "uiRules": {
                "minReviews": 1,
                "hasPopups": true,
                "hasMedia": true
            }
        }
    ]
}